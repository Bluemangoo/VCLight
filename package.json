{
  "name": "vclight-monorepo",
  "private": true,
  "description": "VCLight monorepo pnpm workspace.",
  "devDependencies": {
    "prettier": "^3.2.5"
  },
  "scripts": {
    "build:vclight": "pnpm -C packages/vclight build",
    "build:vclight-router": "pnpm -C packages/vclight-router build",
    "build": "pnpm run /^build:.*/",
    "publish": "pnpm run build&&pnpm -r publish",
    "postinstall": "pnpm run build"
  }
}